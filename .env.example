# Architecture Governance Configuration
# Copy this file to .env and fill in your values

# Required: GitHub Copilot token for authentication
# Get your token from: https://github.com/settings/tokens
COPILOT_TOKEN=your_copilot_token_here

# Optional: LLM model to use (default: claude-opus-4.5)
# Options: claude-opus-4.5, gpt-4, gpt-4-turbo, etc.
MODEL=claude-opus-4.5

# Optional: Enable verbose output (default: true)
VERBOSE=true

# Optional: Default input file (default: docs/sample-architecture.html)
INPUT_FILE=docs/sample-architecture.html
